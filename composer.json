{
  "name": "saro0h/to-do-list",
  "license": "proprietary",
  "type": "project",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.3",
    "doctrine/orm": "^3.3.0",
    "doctrine/doctrine-bundle": "^2.13.0",
    "symfony/monolog-bundle": "^v3.10.0",
    "symfony/polyfill-apcu": "^v1.31.0",
    "doctrine/doctrine-migrations-bundle": "^3.3.1",
    "laminas/laminas-code": "^4.15.1",
    "doctrine/inflector": "^2.0.10",
    "symfony/flex": "^v2.4.7",
    "symfony/asset": "^v6.4.13",
    "symfony/mailer": "6.4.*",
    "symfony/form": "^v6.4.13",
    "symfony/security-bundle": "^v6.4.13",
    "symfony/translation": "^v6.4.13",
    "symfony/validator": "^v6.4.15",
    "doctrine/annotations": "^2.0",
    "symfony/twig-bundle": "^v6.4.13",
    "twig/extra-bundle": "^v3.13.0",
    "twig/twig": "^v3.14.2",
    "symfony/yaml": "^v6.4.13",
    "symfony/webpack-encore-bundle": "^v2.2.0",
    "symfony/cache": "^v6.4.14",
    "symfony/debug-bundle": "6.4.*"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^7.1",
    "symfony/dotenv": "^v6.4.13",
    "symfony/maker-bundle": "^v1.61.0",
    "symfony/stopwatch": "^v6.4.13",
    "symfony/web-profiler-bundle": "^v6.4.14",
    "symfony/browser-kit": "^v6.4.13",
    "doctrine/doctrine-fixtures-bundle": "^3.6"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "6.4.*"
    }
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "platform": {
      "php": "8.3"
    }
  },
  "conflict": {
    "symfony/symfony": "*"
  }
}
